# Example environment variables for feed-management-service
# Copy to .env and customize. Do NOT commit your real .env.

# Server
SERVER_PORT=8090

# Database
SPRING_DATASOURCE_URL=jdbc:postgresql://feed_management_postgres:5432/feed_management_db
SPRING_DATASOURCE_USERNAME=postgres
SPRING_DATASOURCE_PASSWORD=postgres

# Upstream services
USER_SERVICE_URL=http://user-management-service:8082

# Azure Storage (used for media uploads)
AZURE_CONNECTION_STRING=
AZURE_CONTAINER_NAME=
AZURE_ACCOUNT_NAME=
AZURE_SAS_TOKEN=

# JWT
JWT_SECRET=change-me
JWT_HEADER=Authorization
JWT_EXPIRATION=12000000


