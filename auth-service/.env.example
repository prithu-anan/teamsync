# Example environment variables for auth-service
# Copy to .env and customize. Do NOT commit your real .env.

# Server
SERVER_PORT=8081

# Database
SPRING_DATASOURCE_URL=jdbc:postgresql://auth-postgres:5432/auth-db
SPRING_DATASOURCE_USERNAME=postgres
SPRING_DATASOURCE_PASSWORD=postgres

# JWT
JWT_SECRET=change-me
JWT_HEADER=Authorization
JWT_EXPIRATION=12000000

# Service URLs
USER_MANAGEMENT_SERVICE_URL=http://user-management-service:8082


